{
  "extends": "@calcom/tsconfig/nextjs.json",
  "compilerOptions": {
    "strict": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@api/*": [
        "pages/api/*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@/*": [
        "*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "../../packages/types/next-auth.d.ts"
  ],
  "exclude": [
    "node_modules",
    "templates",
    "auth"
  ]
}
