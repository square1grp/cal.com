{
  "baseBranch": "origin/main",
  "globalDependencies": [".env"],
  "pipeline": {
    "@calcom/web#db-deploy": {},
    "@calcom/web#db-seed": {
      "dependsOn": ["@calcom/web#db-deploy"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "db-seed": {},
    "deploy": {
      "dependsOn": []
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "dx": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "start": {},
    "test": {
      "dependsOn": ["@calcom/web#build", "@calcom/web#db-seed"]
    },
    "type-check": {
      "outputs": []
    }
  }
}
